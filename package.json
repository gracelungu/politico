{
  "name": "politico",
  "version": "1.0.0",
  "description": "A web-application that enables citizens give their mandate to politicians running for different government offices",
  "main": "app.js",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/cli": "^7.2.3",
    "@babel/preset-env": "^7.3.1",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "eslint-config-airbnb": "^17.1.0",
    "express": "^4.16.4",
    "karma": "^4.0.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^2.0.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "istanbul": "^1.0.0-alpha.2",
    "jasmine": "^3.3.1",
    "jasmine-node": "^1.16.2",
    "jest": "^24.0.0",
    "nyc": "^13.2.0"
  },
  "scripts": {
    "test-cov": "npm run build && node_modules/.bin/babel-node node_modules/.bin/istanbul cover jasmine",
    "test": "./node_modules/.bin/eslint V1 --fix && npm run test-cov ",
    "coveralls": "istanbul cover jasmine --captureExceptions && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build": "./node_modules/.bin/babel V1 app.js",
    "start": "npm run build && babel-node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gracelungu/politico.git"
  },
  "author": "Grace lungu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gracelungu/politico/issues"
  },
  "homepage": "https://github.com/gracelungu/politico#readme"
}
